# WebView Optimized Configuration for Capacitor Apps
REACT_APP_API_URL=https://lottery-backend-l1k7.onrender.com
REACT_APP_API_VERSION=v1
REACT_APP_VERSION=2.0.0
REACT_APP_ENVIRONMENT=production

# Performance Optimizations for WebView
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false
IMAGE_INLINE_SIZE_LIMIT=8192

# PWA Configuration - Disabled for native webview
REACT_APP_PWA_ENABLED=false
REACT_APP_OFFLINE_ENABLED=true

# Mobile Optimizations - Aggressive for webview
REACT_APP_MOBILE_TIMEOUT=30000
REACT_APP_RETRY_ATTEMPTS=2
REACT_APP_RETRY_DELAY=1500

# Debug Settings - Disabled for production
REACT_APP_DEBUG_API=false
REACT_APP_DEBUG_NETWORK=false

# Bundle Optimization
REACT_APP_ENABLE_PROFILING=false
REACT_APP_LAZY_LOAD=true
